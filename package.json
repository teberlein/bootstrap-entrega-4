{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "proyecyo node js",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\"  -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin precios.html -d dist --htmlmin -o dist/precios.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin tyc.html -d dist --htmlmin -o dist/tyc.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tatoeberlein@bitbucket.org/tatoeberlein/guia-hoteles.git"
  },
  "author": "Tom√°s Eberlein",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/tatoeberlein/guia-hoteles/issues"
  },
  "homepage": "https://bitbucket.org/tatoeberlein/guia-hoteles#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "node-sass": "^9.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^5.0.5",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "jquery": "^3.7.1",
    "popper.js": "^1.16.1",
    "sass": "^1.70.0"
  }
}
